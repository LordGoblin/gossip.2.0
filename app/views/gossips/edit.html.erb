<h1>coucou ici edit</h1>

<%= form_for @gossip do |f| %>

	<div class="form-group">
		<label>titre du gossip</label>
		<%= f.text_field :title, class: 'form-control' %>
	</div>
	<div class="form-group">
		<label>content du gossip</label>
		<%= f.text_area :content, class: 'form-control' %>
	</div>
	<div class="form-group">
		<button type="submit" class="btn btn-primary">modifier gossip</button>
	</div>

<% end %>